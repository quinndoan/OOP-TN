<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="globalict.aims.screen.controller.ItemController" prefHeight="175.0" prefWidth="305.0">
   <children>
      <VBox alignment="CENTER" prefHeight="175.0" prefWidth="305.0" spacing="10.0">
         <children>
            <Label fx:id="lblTitle" alignment="CENTER" prefHeight="50.0" prefWidth="305.0" />
            <Label fx:id="lblCost" alignment="CENTER" prefHeight="50.0" prefWidth="305.0" />
            <HBox alignment="CENTER" prefHeight="50.0" prefWidth="305.0" spacing="20.0">
               <children>
                  <Button fx:id="btnAddToCart" text="Add to Cart" onAction="#btnAddToCartClicked" prefHeight="50.0" prefWidth="305.0" />
                  <Button fx:id="btnPlay" text="Play" onAction="#btnPlayClicked" prefHeight="50.0" prefWidth="305.0" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
